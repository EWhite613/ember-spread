{
  "name": "ember-spread",
  "version": "2.0.0",
  "description": "Dynamic options for dynamic components",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": "git@github.com:ciena-blueplanet/ember-spread.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Steven Glanzer (https://github.com/sglanzer)",
  "contributors": [
    "Matthew Dahl (https://github.com/sandersky)",
    "Michael Carroll (https://github.com/juwara0)"
  ],
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "2.16.1",
    "ember-cli-chai": "0.4.2",
    "ember-cli-code-coverage": "0.3.5",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-frost-blueprints": "2.0.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-snippet": "^1.10.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-frost-test": "^2.1.2",
    "ember-hook": "^1.4.2",
    "ember-load-initializers": "^1.0.0",
    "ember-prop-types": "^4.0.0",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.16.0",
    "ember-test-utils": "^4.3.2",
    "eslint-plugin-ember": "^4.5.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.13.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "babel-plugin-ember-modules-api-polyfill": "^2.0.1",
    "ember-cli-babel": "^6.6.0"
